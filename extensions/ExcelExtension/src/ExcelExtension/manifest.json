{
  "name": "ExcelExtension",
  "displayName": "%Application.Name%",
  "description": "%Application.Description%",
  "version": "1.1.0",
  "publisher": "DENSO CREATE INC.",

  "main": "ExcelExtension.dll",
  "lifecycle": "project",
  "baseprofile": "車載システムソフト開発",

  "extensionPoints": {
    "commands": [
      {
        "id": "ExcelExtension.Import",
        "execFunc": "Import"
      },
      {
        "id": "ExcelExtension.Export",
        "execFunc": "Export"
      }
    ],

    "ribbon": {
